# 集群ID（必填）
CLUSTER_ID=your_cluster_id

# 集群密钥（必填）
CLUSTER_SECRET=your_cluster_secret

# 集群IP（可选，不填会自动获取）
# CLUSTER_IP=8.8.8.8

# 集群端口（默认4000）
# CLUSTER_PORT=4000

# 集群公开端口（默认与CLUSTER_PORT相同）
# CLUSTER_PUBLIC_PORT=4000

# 是否使用自己的证书
# CLUSTER_BYOC=false

# 禁用访问日志
# DISABLE_ACCESS_LOG=false

# 启用Nginx
# ENABLE_NGINX=false

# 启用UPnP
# ENABLE_UPNP=true

# 存储类型（默认file）
# CLUSTER_STORAGE=file

# 存储选项（JSON格式）
# CLUSTER_STORAGE_OPTIONS={"baseURL":"http://example.com/webdav","username":"user","password":"pass"}

# SSL证书和密钥路径
# SSL_KEY=/path/to/key.pem
# SSL_CERT=/path/to/cert.pem

# 是否以单进程模式运行（不使用cluster）
# NO_DAEMON=true 